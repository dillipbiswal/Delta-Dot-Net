<?xml version="1.0" encoding="UTF-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
  <Fragment>
    <UI>
      <Dialog Id="CustomPropsDlg" Width="370" Height="270" Title="!(loc.InstallDirDlg_Title)">
        <Control Id="Next" Type="PushButton" X="236" Y="243" Width="56" Height="17" Default="yes" Text="!(loc.WixUINext)" />
        <Control Id="Back" Type="PushButton" X="180" Y="243" Width="56" Height="17" Text="!(loc.WixUIBack)" />
        <Control Id="Cancel" Type="PushButton" X="304" Y="243" Width="56" Height="17" Cancel="yes" Text="!(loc.WixUICancel)">
          <Publish Event="SpawnDialog" Value="CancelDlg">1</Publish>
        </Control>

        <Control Id="Description" Type="Text" X="25" Y="23" Width="280" Height="15" Transparent="yes" NoPrefix="yes" Text="Enter the Tenant GUID, Customer GUID (optional) and Server GUID (optional)" />
        <Control Id="Title" Type="Text" X="15" Y="6" Width="200" Height="15" Transparent="yes" NoPrefix="yes" Text="Tenant, Customer and Server" />
        <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="44" TabSkip="no" Text="!(loc.InstallDirDlgBannerBitmap)" />
        <Control Id="BannerLine" Type="Line" X="0" Y="44" Width="370" Height="0" />
        <Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="0" />
        <Control Id="TenantGuidDescription" Type="Text" X="5" Y="50" Width="220" Height="20" Transparent="yes" NoPrefix="yes" Text="Tenant GUID:" />
        <Control Id="TenantGuid" Type="Edit" X="5" Y="65" Width="220" Height="20" Property="TENANTID" Text="1A19A18A-846C-49DA-93C1-8948AFDC0151"/>
        <Control Id="CustomerGuidDescription" Type="Text" X="5" Y="90" Width="220" Height="20" Transparent="yes" NoPrefix="yes" Text="Customer GUID:" />
        <Control Id="CustomerGuid" Type="Edit" X="5" Y="105" Width="220" Height="20" Property="CUSTOMERID" />
        <Control Id="ServerGuidDescription" Type="Text" X="5" Y="130" Width="220" Height="20" Transparent="yes" NoPrefix="yes" Text="Server GUID:" />
        <Control Id="ServerGuid" Type="Edit" X="5" Y="145" Width="220" Height="20" Property="SERVERID" />
      </Dialog>
    </UI>
  </Fragment>
</Wix>