@model Status

@{
    var allValues = Enum.GetValues(typeof(Status)).Cast<Status>();
    allValues = allValues.Where(x => x == Status.Active || x == Status.InMaintenance).ToArray();
   
}

@Html.DropDownListFor(m => m, allValues
    .Select(m => {
        string enumVal = Enum.GetName(Model.GetType(), m);
        return new SelectListItem() {
            Selected = (Model.ToString() == enumVal),
            Text = enumVal,
            Value = enumVal
        };
    }))

